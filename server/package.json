    "@aws-sdk/client-s3": "^3.400.0",
    "@azure/arm-compute": "^20.0.0",
    "@azure/identity": "^3.4.0",
    "@google-cloud/compute": "^3.0.0",
    "aws-sdk": "^2.1410.0",
    "axios": "^1.4.0",
    "bull": "^4.10.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ethers": "^6.4.0",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
